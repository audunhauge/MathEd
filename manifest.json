{
    "short_name": "MathEd",
    "name": "Math Editor",
    "start_url": "/index.html",
    "background_color": "#3367D6",
    "display": "standalone",
    "scope": "/",
    "theme_color": "#3367D6",
    "description": "Math Editor",
    "share_target": {
        "action": "/edit.html",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
          "title": "name",
          "text": "description",
          "url": "link",
          "files": [
            {
              "name": "tests",
              "accept": ["text/csv", ".mtx"]
            }
          ]
        }
      },
    "icons": [
        {
            "src": "media/maed-128.png",
            "sizes": "128x128",
            "type": "image/png"
        },
        {
            "src": "media/maed-144.png",
            "sizes": "144x144",
            "type": "image/png"
        }
    ],
    "file_handlers": [
        {
          "action": "/open-mtx",
          "accept": {
            "text/plain": [".mtx"]
          },
          "icons": [
            {
              "src": "mtx-icon.png",
              "sizes": "256x256",
              "type": "image/png"
            }
          ]
        }
      ]
}